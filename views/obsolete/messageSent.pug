extends layout.pug

prepend styles
    link(rel="stylesheet", href="/styles/messageSent.css")

prepend scripts
    //-script(src="/scripts/FILENAME.js")


block content
    //-CONTENT-STARTS-HERE-//

    section#message
        h2 Your message has been sent and we will get back to you asap
        h3 Name
        h4#name=name
        h3 Email
        h4#email=email
        h3 Subject
        h4#subject=subject
        h3 Message
        h4#message=message
        form(action="/")
            input(type="submit" value="Return Home")